{
  "name": "memoize-function",
  "version": "1.0.0",
  "description": "A memoization library that caches all results and supports *N* arguments with any type.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverage",
    "open:coverage": "open ./coverage/lcov-report/src/index.html",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .ts \"src/**/*\"",
    "format": "prettier --write \"src/**/*\""
  },
  "keywords": [
    "cache",
    "memoize",
    "function",
    "memoization",
    "performance",
    "memoize-function"
  ],
  "author": "Paweł Wojtasiński",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
